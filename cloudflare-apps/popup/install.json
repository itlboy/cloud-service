{
    "resources": {
        "body": [
            {
                "type": "style",
                "src": "./source/app.css"
            },
            {
                "type": "script",
                "src": "./source/app.js"
            }
        ]
    },
    "preview": {
        "handlers": [
            {
                "options": ["*"],
                "execute": "INSTALL_SCOPE.setOptions(INSTALL_OPTIONS)",
                "reload": false
            }
        ]
    },
    "options": {
        "properties": {
            "color": {
                "default": "#f38020",
                "format": "color",
                "title": "Color Example",
                "type": "string"
            },
            "backgroundImage": {
                "title": "background image",
                "type": "string",
                "format": "image"
            }
        }
    }
}
